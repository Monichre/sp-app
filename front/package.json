    "babel-plugin-import": "^1.12.0",
    "color": "^3.1.2",
    "customize-cra": "^0.5.0",
    "firebase": "^5.9.0",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "grommet-icons": "^4.2.0",
    "less": "^3.9.0",
