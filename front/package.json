{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "quokka": {
    "babel": {
      "presets": [
        "react-app"
      ]
    }
  },
  "scripts": {
    "check:target": "../bin/check-target.sh",
    "start": "TARGET=local && source ./scripts/local-env.sh && react-scripts start",
    "start:other": "yarn check:target && source ./scripts/ssm-env.sh && react-scripts start",
    "build": "yarn check:target && source ./scripts/ssm-env.sh && react-scripts build",
    "build:test": "TARGET=test yarn build",
    "deploy": "yarn check:target && sls deploy && sls info --verbose | grep SiteUrl",
    "deploy:test": "TARGET=test yarn deploy",
    "e2e": "jest",
    "eject": "react-scripts eject",
    "": "",
    "cosmos": "cosmos",
    "cosmos:export": "cosmos-export"
  },
  "dependencies": {
    "@ant-design/dark-theme": "^0.2.2",
    "@types/lodash": "^4.14.137",
