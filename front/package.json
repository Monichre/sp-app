    "react-helmet": "^5.2.0",
    "react-lifecycle-visualizer": "^2.2.4",
    "react-markdown": "^4.0.8",
    "react-moment": "^0.8.4",
    "react-responsive": "^6.1.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "react-spring": "^8.0.27",
    "react-tooltip": "^3.10.0",
    "react-use-gesture": "^5.2.4",
    "rebass": "^4.0.2",
    "recharts": "^1.5.0",
    "resize-observer-polyfill": "^1.5.1",
    "sanitize.css": "^8.0.0",
    "styled-components": "^4.1.3",
    "typescript": "3.3.3333",
    "victory": "^32.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/firebase": "^3.2.1",
