    "serverless-offline": "^4.8.1",
    "serverless-offline-kinesis": "^1.5.1",
    "serverless-offline-scheduler": "^0.3.7",
    "serverless-offline-sqs": "^1.6.1",
    "serverless-plugin-epsagon": "^1.5.2",
